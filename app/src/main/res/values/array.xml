<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Temperature String Arrays for Preferences -->
    <string-array name="temperature_units">
        <item>@string/Celsius</item>
        <item>@string/Fahrenheit</item>
        <item>@string/Kelvin</item>
    </string-array>

    <string-array name="temperature_units_values">
        <item>C°</item>
        <item>F°</item>
        <item>K°</item>
